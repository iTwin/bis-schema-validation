{
  "name": "@bentley/native-schema-locater",
  "description": "Contains an XML schema file locater which uses the native addon to deserialize EC 3.1 schemas",
  "license": "MIT",
  "version": "0.2.178",
  "main": "./lib/native-schema-locater.js",
  "typings": "./lib/native-schema-locater",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/bis-schema-validation"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint -f visualstudio \"./src/**/*.ts\" 1>&2",
    "beforeBuild": "npm run clean && npm run lint",
    "build": "npm run beforeBuild && tsc && npm run copy:test-assets",
    "test": "mocha",
    "test:watch": "betools test --watch",
    "cover": "nyc npm -s test",
    "copy:test-assets": "cpx \"./src/test/assets/**/*\" ./lib/test/assets"
  },
  "keywords": [
    "Bentley",
    "iModel",
    "iModelJs",
    "EC"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "devDependencies": {
    "@itwin/eslint-plugin": "^4.0.2",
    "@itwin/build-tools": "4.8.0-dev.6",
    "@types/chai": "4.3.1",
    "@types/chai-as-promised": "^7",
    "@types/fs-extra": "^5.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "~18.16.20",
    "@types/rimraf": "^2.0.2",
    "@types/sinon": "^7.0.5",
    "chai": "^4.3.10",
    "chai-as-promised": "^7",
    "cpx2": "^3.0.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^7.1.1",
    "typescript": "~5.0.2",
    "source-map-support": "^0.5.9",
    "eslint": "^8.44.0"
  },
  "peerDependencies": {
    "@itwin/ecschema-metadata": "4.8.0-dev.6",
    "@itwin/ecschema-locaters": "4.8.0-dev.6"
  },
  "dependencies": {
    "@itwin/core-bentley": "4.8.0-dev.6",
    "@itwin/core-geometry": "4.8.0-dev.6",
    "@itwin/imodels-access-backend": "4.0.0",
    "@itwin/core-backend": "4.8.0-dev.6",
    "@itwin/core-quantity": "4.8.0-dev.6",
    "@itwin/core-common": "4.8.0-dev.6",
    "@xmldom/xmldom": "~0.8.5",
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "fs-extra": "^7.0.1"
  }
